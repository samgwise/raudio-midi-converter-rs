{
  "name": "csv-to-midi-web",
  "version": "1.0.0",
  "description": "CSV to MIDI Converter - Web Application deployed on Cloudflare Workers",
  "main": "src/worker.js",
  "scripts": {
    "build": "npm run build:wasm",
    "build:wasm": "wasm-pack build csv-to-midi-wasm --target web --out-dir ../www/pkg --release",
    "build:ci": "npm run install:wasm-pack && npm run build:wasm",
    "install:wasm-pack": "curl -sSf https://rustwasm.github.io/wasm-pack/installer/init.sh | sh",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env=''",
    "deploy:quick": "wrangler deploy --env='' --no-bundle",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "preview": "wrangler preview",
    "logs": "wrangler tail",
    "logs:staging": "wrangler tail --env staging",
    "logs:production": "wrangler tail --env production",
    "wasm:install": "curl https://rustwasm.github.io/wasm-pack/installer/init.sh -sSf | sh",
    "serve:local": "cd www && python -m http.server 8000"
  },
  "keywords": [
    "csv",
    "midi",
    "audio",
    "music",
    "wasm",
    "rust",
    "cloudflare-workers"
  ],
  "author": "Your Name",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "wrangler": "^4.35.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/csv-to-midi"
  },
  "bugs": {
    "url": "https://github.com/yourusername/csv-to-midi/issues"
  },
  "homepage": "https://github.com/yourusername/csv-to-midi#readme"
}
